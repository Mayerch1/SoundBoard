<Window x:Class="DicsordBot.PlaylistAddDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DicsordBot"
        mc:Ignorable="d"
        Title="PlaylistAddDialog" Height="100" Width="600" ResizeMode="NoResize" WindowStyle="None">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="6*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0">
            <TextBox x:Name="box_Name" Text="Name" />
        </Grid>
        <Grid Grid.Column="1">
            <Button Content="Accept" Click="btn_Accept_Click" />
        </Grid>
    </Grid>
</Window>