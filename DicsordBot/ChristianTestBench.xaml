<UserControl x:Class="DicsordBot.ChristianTestBench"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:DicsordBot"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <Slider Minimum="0" Maximum="100" Value="{Binding Volume}" />
        </Grid>
        <Grid Grid.Row="1">
            <TextBox Text="{Binding Volume}" />
        </Grid>
        <Grid Grid.Row="2">
            <Slider Minimum="0" Maximum="10000" Value="{Binding Volume}" />
        </Grid>
    </Grid>
</UserControl>